{
  "_version": "1.59.0",
  "sap.app": {
    "id": "com.sap.demo.managecustomer.managecustomer",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.3-pre-20240717081120-23681868d.0",
      "toolsId": "3f59f18f-0cb5-48f4-80f2-0e3fde6da662"
    },
    "dataSources": {
      "mainService": {
        "uri": "/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "CustomerDetails-display": {
          "semanticObject": "CustomerDetails",
          "action": "display",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.127.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": false,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.demo.managecustomer.managecustomer.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.sap.demo.managecustomer.managecustomer.view",
        "controlAggregation": "pages",
        "controlId": "idApp",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteCustomers",
          "pattern": ":?query:",
          "target": [
            "TargetCustomers"
          ]
        },
        {
          "name": "RouteCustomerDetail",
          "pattern": "Customers({CustomerID})",
          "target": [
            "TargetCustomerDetail"
          ]
        }
      ],
      "targets": {
        "TargetCustomers": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Customers",
          "viewName": "Customers"
        },
        "TargetCustomerDetail": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CustomerDetail",
          "viewName": "CustomerDetail"
        }
      }
    },
    "rootView": {
      "viewName": "com.sap.demo.managecustomer.managecustomer.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
